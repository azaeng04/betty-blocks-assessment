  "scripts": {
    "cy:open": "cypress open --project .",
    "cy:run": "PERCY_BRANCH=local percy exec -- cypress run",
    "cy:run:ci": "PERCY_BRANCH=master percy exec -- cypress run",
    "cy:verify": "cypress verify",
    "test:chrome": "yarn cy:run -b chrome --headless",
    "test:firefox": "yarn cy:run -b firefox --headless",
    "test:chrome:ci": "yarn cy:run:ci -b chrome --headless",
    "test:firefox:ci": "yarn cy:run:ci -b firefox --headless"
  },
  "optionalDependencies": {
    "cypress": "^8.1.0"
  },
  "devDependencies": {
    "@percy/cli": "^1.0.0-beta.62",
